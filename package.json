  "dependencies": {
    "bcrypt": "^5.1.1",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.2",
    "react-router-dom": "^6.26.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cypress": "^13.13.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4"
  }
}
